<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="529.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.lapr2_interfacegrafica.ui.CheckPerformanceIndicatorsUI">
   <children>
      <BorderPane prefHeight="530.0" prefWidth="800.0" AnchorPane.topAnchor="0.0">
         <center>
            <BorderPane prefHeight="500.0" prefWidth="800.0" BorderPane.alignment="CENTER">
               <center>
                  <HBox prefHeight="295.0" prefWidth="800.0" BorderPane.alignment="CENTER">
                     <children>
                        <ListView fx:id="lstFreelancerAndInfo" prefHeight="250.0" prefWidth="400.0" />
                        <BarChart fx:id="barChartHistograms" prefHeight="250.0" prefWidth="400.0">
                          <xAxis>
                            <CategoryAxis fx:id="intervals" label="Intervals" side="BOTTOM" />
                          </xAxis>
                          <yAxis>
                            <NumberAxis fx:id="numberOfFreelancers" label="Number of freelancers" side="LEFT" />
                          </yAxis>
                        </BarChart>
                     </children>
                  </HBox>
               </center>
               <bottom>
                  <HBox prefHeight="92.0" prefWidth="800.0" BorderPane.alignment="CENTER">
                     <children>
                        <TextField fx:id="txtProbability" prefHeight="58.0" prefWidth="674.0">
                           <HBox.margin>
                              <Insets left="55.0" top="20.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
               </bottom>
            </BorderPane>
         </center>
         <top>
            <Label fx:id="lblCheckPerformance" prefHeight="55.0" prefWidth="338.0" text="Check Performance Indicators" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" top="5.0" />
               </padding>
            </Label>
         </top>
         <bottom>
            <HBox prefHeight="57.0" prefWidth="800.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btnShowHdelays" mnemonicParsing="false" onAction="#showHdelays" text="Show Histograms Delays">
                     <HBox.margin>
                        <Insets left="55.0" top="13.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnDeviationDelay" mnemonicParsing="false" onAction="#showHpayments" text="Show Histograms Payments">
                     <HBox.margin>
                        <Insets left="55.0" top="13.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnClean" mnemonicParsing="false" onAction="#cleanAll" text="Clean">
                     <HBox.margin>
                        <Insets left="55.0" top="13.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btnReturn" mnemonicParsing="false" onAction="#returnAction" text="Return">
                     <HBox.margin>
                        <Insets left="55.0" top="13.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
